Expression_for_v2 <: Expression_for_v1 {
  DereferenceExpression := #("%") Expression #("%")

  stringLiteral
    := "\"" doubleCharacter* "\""
     | "'" singleCharacter* "'"
  doubleEscapeSequence := "`\"" | commonEscapeSequence
  singleEscapeSequence = "\\'" | commonEscapeSequence
  singleCharacter
    = ~("'" | "`" | lineTerminator) any
    | doubleEscapeSequence
}